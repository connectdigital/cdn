<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta http-equiv="Content-Language" content="tr" />
	<meta http-equiv="cache-control" content="no-cache" />
	<meta http-equiv="pragma" content="no-cache" />
	<title></title>
</head>
<body>
	<style>      
	body {margin: 0px}      
	.container { background-color:#4a5256;  width:728px; height:90px; position:relative; overflow:hidden; border:1px solid #000;}      	   
	.playz_gallery{ margin:auto; display:block; }
	.mgnews{ position:relative; margin:auto; overflow:hidden; }

	.box{ display:inline-block; text-align:center; position:relative; width:190px; height:90px; margin:0 10px 0 0; }

	.box:last-child{ margin:0px; }
	
	.box:first-child{ width:100px; margin:0px; }
	
	.box a{ font-family:Arial, Helvetica, sans-serif;color:#fff; text-decoration:none; display:block; font-size:12px; height:100%;}
	.box a img{ position:absolute; text-align:center;}
	.box a span{ font-size:10px; padding:5px 0px; display:block; overflow:hidden; position:absolute; left:0px; bottom:0px; width:100%; text-align:center; background:#232323;}
	.box .box-title{ position:absolute; width:100%; min-height:20px; left:0; bottom:0;}
	.box .box-image{ position:relative; width:100%; height:100%; background-repeat: no-repeat; background-size:100%; background-position:center;}
	
	.box:first-child .box-image{ background-size:93%; }
	.box:first-child .box-image div{ position: absolute; bottom:0px; left:0; background:#232323; width:100%;}
	
	</style>
	
	<div class="container">
		<div class="playz_gallery">
			<div class="mgnews">		
				<div class="box">
					<a target="_blank" href="http://www.playz.com/?utm_source=r10&utm_medium=728x90&utm_campaign=r10-728x90">					
						<div class="box-image" style="background-image:url(data:image/png;base64,)">
							<div class="box-title"><span>www.playz.com</span></div>
						</div>
					</a>
				</div>	
			</div>		
			<a target="_blank" href="http://www.playz.com/?utm_source=r10&utm_medium=728x90&utm_campaign=r10-728x90">
				<img src="data:image/png;base64," style="position: absolute;right: 0;top: 27px;">
			</a>
		</div>	
	</div>
	
	<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.1/jquery.min.js"></script>
	<script>
		function template(data)
		{
			return '<div class="box">'+
					'<a target="_blank" href="'+data.url+'">'+
						'<div class="box-image" style="background-image:url('+data.img+')"></div>'+
						'<div class="box-title"><span>'+data.title+'</span></div>'+
					'</a>'+
					'</div>';
		}
		
		$(document).ready(function(){		
			$.ajax({
				type	: "GET",
				url		: "data.json",
				cache 	: false,
				success	: function(response){					
					try{
						$(response).each(function(i,value){
							$('.mgnews').append( template(value) )
						});
					}
					catch(err){
						console.log("Playz Error !", err);
					}
				},
				error: function (xhr, ajaxOptions, thrownError) {
					console.log("Playz Error !", xhr.status,thrownError);
				}
			});
		});	
	</script>

</body>
</html
